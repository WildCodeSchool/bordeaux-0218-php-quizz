{% extends 'layout.html.twig' %}

{% block content %}
    <h1>Users</h1>
    <ul>
        {% for user in users %}
            <li><a href="/user/{{ users.id }}">{{ users.id }}. {{ users.firstName }}</a></li>
        {% else %}
            <li>Nothing to display</li>
        {% endfor %}
    </ul>
{% endblock %}
