{% extends 'layout.html.twig' %}

{% block content %}

<h1>Questions</h1>
<ul>
	{% for question in questions %}
    	<li>{{ question.questionName }}
    	<ul> 
	    	{% for answer in answers %}
	    			<li>{{ answer.name }}</li>
	    	{% else %}
	        <li>Nothing to display</li>
    		{% endfor %}
    	</ul>
    	</li>
    {% else %}
        <li>Nothing to display</li>
    {% endfor %}
</ul>



{% endblock %}